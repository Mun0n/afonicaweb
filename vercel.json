{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/shop/(.*)",
      "dest": "https://afonicanaranjo.com/shop/$1",
      "continue": true
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "ignoreRoutes": [
    "/shop/(.*)"
  ]
} 