{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/shop/(.*)",
      "headers": [
        {
          "key": "x-middleware-skip",
          "value": "1"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/shop/:path*",
      "destination": "https://afonicanaranjo.com/shop/:path*"
    }
  ]
} 